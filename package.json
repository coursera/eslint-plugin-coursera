{
  "name": "eslint-plugin-coursera",
  "version": "0.0.3",
  "description": "Custom ESLint rules made by Coursera developers for smoother development",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    }
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "loose": true
        }
      ]
    ]
  },
  "author": "Lewis Chung",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "rm -rf lib && babel src -d lib",
    "test": "mocha tests --recursive --compilers js:babel-register",
    "format": "prettier-eslint --print-width=120"
  },
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coursera/eslint-plugin-coursera"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "eslint": "~2.6.0",
    "lodash.defaults": "^4.2.0",
    "mocha": "^2.4.5",
    "prettier": "^1.14.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  },
  "engines": {
    "node": ">=4"
  },
  "license": "Apache-2.0"
}
